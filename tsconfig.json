{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "rootDir": ".",
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "sourceMap": false,
    "declaration": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "module": "esnext",
    "jsx": "react",
    "jsxFactory": "h",
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "stripInternal": true,
    "importHelpers": true,
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "include": [
    "declarations.d.ts",
    "src"
  ]
}
